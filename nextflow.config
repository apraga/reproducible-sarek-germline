igenomes_base = ''
params {
	genomes {
		'GRCh38' {
			bwa = "dgenomes/genome-human"
			dbsnp = "dgenomes/dbsnp/GCF_000001405.40_chr.gz"
			dbsnp_tbi = "dgenomes/dbsnp/GCF_000001405.40_chr.gz.tbi"
			dbsnp_vqsr = ""
			dict = ""
			fasta = "dgenomes/genome-human/GCA_000001405.15_GRCh38_full_analysis_set.fna"
			fasta_fai = "dgenomes/genome-human/GCA_000001405.15_GRCh38_full_analysis_set.fna.fai"
			germline_resource = ""
			germline_resource_tbi = ""
			known_snps = ""
			known_snps_tbi = ""
			known_snps_vqsr = ""
			known_indels = ""
			known_indels_tbi = ""
			known_indels_vqsr = ""
			snpeff_db = ""
			snpeff_genome = ""
		}
	}
	genome = 'GRCh38'
}
process {
        // Force strelka to use python2 shipped with nix
        withName: '.*STRELKA.*' {
            // We get the path form the first line of a file packaged by nix
            beforeScript = 'STRELKA_SCRIPT=$(which configureStrelkaGermlineWorkflow.py 2>/dev/null) && [ -f "$STRELKA_SCRIPT" ] && export PATH="$(head -n1 "$STRELKA_SCRIPT" | sed "s|^#!\\(.*\\)/python2.7|\\1|"):$PATH"'
        }
        // Force manta to use python2 shipped with nix
        withName: '.*MANTA.*' {
            // We get the path form the first line of a file packaged by nix
            beforeScript = 'MANTA_SCRIPT=$(which configManta.py 2>/dev/null) && [ -f "$MANTA_SCRIPT" ] && export PATH="$(head -n1 "$MANTA_SCRIPT" | sed "s|^#!\\(.*\\)/python2|\\1|"):$PATH"'
        }
}
